{
  "name": "aspiretest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android --appIdSuffix=development",
    "ios": "react-native run-ios --scheme '[DEV] AspireTest'",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:prod": "npx react-native run-android --variant=release",
    "android-build:prod": "cd android && ./gradlew assembleRelease && cd ..",
    "start": "react-native start",
    "start:pure": "npx react-native start --reset-cache",
    "postinstall": "run-script-os",
    "postinstall:macos": "patch-package && cd ios && pod install && cd ..",
    "postinstall:default": "patch-package",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx --fix --max-warnings 0"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "i18next": "^21.9.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nativewind": "2.0.10",
    "qs": "^6.11.0",
    "react": "18.1.0",
    "react-hook-form": "^7.35.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.4",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.3.0",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-fast-image": "^8.6.1",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-localize": "^2.2.3",
    "react-native-modal": "^13.0.1",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-permissions": "^3.6.1",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.1.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-navigation/devtools": "^6.0.9",
    "@testing-library/jest-native": "^4.0.11",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/color": "^3.0.3",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.16",
    "@types/qs": "^6.9.7",
    "@types/react-native": "^0.69.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "axios-mock-adapter": "^1.21.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-testing-library": "^5.6.3",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "patch-package": "^6.4.7",
    "react-native-flipper": "^0.163.0",
    "react-test-renderer": "18.1.0",
    "redux-flipper": "^2.0.2",
    "rn-async-storage-flipper": "^0.0.10",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "./jest-setup.tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|@react-native-community|@react-navigation)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/components/common/Icon/getIconType.ts",
      "/src/containers/screens/Main/Home/Example/",
      "/src/components/common/BottomSheet/components/",
      "/src/core/configs",
      "/src/core/plugins",
      "/src/core/utils/Redux.ts"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.ts"
    }
  }
}
